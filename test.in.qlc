#include <stdint.h>
#include <stdio.h>

typedef struct MyStruct {
  int32_t x, y, z;

  void print(MyStruct *self) {
    printf("(%i, %i, %i)", self->x, self->y, self->z);
  }
} MyStruct;

void someFunction() {
  printf("Hello, World!\n");
}

int main(void) {
  someFunction();
  MyStruct s = (MyStruct){.x = 10, .y = 20, .z = 30};
  s.print();
  printf("\n");
  return 0;
}
