#include <stdint.h>
#include <stdio.h>

typedef struct MyStruct {
  int32_t x, y, z;
  char c;

  void print(MyStruct *self);
} MyStruct;

void MyStruct.print(MyStruct *self) {
  printf("(%i, %i, %i)", self->x, self->y, self->z);
}

void someFunction() { printf("Hello, World!\n"); }

int main(void) {
  someFunction();
  MyStruct s = (MyStruct){.x = 0b10, .y = 0xFF, .z = 0o30, .c = '\n'};
  s.print();
  MyStruct.print(&s);
  printf("\n");
  return 0;
}
